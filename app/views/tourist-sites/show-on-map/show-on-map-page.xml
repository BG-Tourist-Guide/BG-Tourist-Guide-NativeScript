<Page xmlns="http://schemas.nativescript.org/tns.xsd" xmlns:maps="nativescript-google-maps-sdk" loaded="pageLoaded" navigatingTo="pageNavigatingTo" actionBarHidden="true">
  <StackLayout padding="10">
      <maps:mapView latitude="{{ latitude }}" 
        longitude="{{ longitude }}" 
        mapReady="onMapReady"
        markerSelect="onMarkerSelect" 
        cameraChanged="onCameraChanged" />
  </StackLayout>
</Page>
