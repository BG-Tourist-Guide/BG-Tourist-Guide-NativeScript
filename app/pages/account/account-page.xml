<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <Page.actionBar>
      <ActionBar title="{{ title }}" />
  </Page.actionBar>
  <StackLayout>
    <Label text="Account" class="title" />
    <Button class="btn-success" text="Login" tap="loginBtnTap" />
    <Button class="btn-primary" text="Register" tap="registerBtnTap" />
  </StackLayout>
</Page>
