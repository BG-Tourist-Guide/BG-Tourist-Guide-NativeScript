<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" actionBarHidden="true">
  <StackLayout>
    <Label class="title" text="{{ title }}" />
    <Button class="btn-success" text="Near me" tap="nearMeBtnTap" />
    <Button class="btn-success" text="All tourist sites" tap="allBtnTap" />
    <Button class="btn-success" text="Official tourist sites" tap="officialBtnTap" />
    <Button class="btn-success" text="Unofficial tourist sites" tap="unofficialBtnTap" />
    <Button class="btn-success" text="Logout" tap="logoutBtnTap" />
  </StackLayout>
</Page>
